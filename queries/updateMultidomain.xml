<query id="updateMultidomain" action="update">
	<tables>
		<table name="multidomain" />
	</tables>
	<columns>
		<column name="domain" var="domain" />
		<column name="module_srl" var="module_srl" filter="number" />
        <column name="document_srl" var="document_srl" filter="number" default="0" />
	</columns>
	<conditions>
		<condition operation="equal" column="multidomain_srl" var="multidomain_srl" filter="number" />
	</conditions>
</query>